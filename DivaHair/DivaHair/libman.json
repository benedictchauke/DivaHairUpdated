{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "files": [ "jquery.min.js", "jquery.slim.js", "jquery.slim.min.map"],
      "destination": "wwwroot/lib/jquery"
    },

    {
      "library": "bootstrap@5.3.3",
      "files": [ "css/bootstrap-grid.css", "css/bootstrap-grid.css.map", "css/bootstrap-grid.min.css" ],
      "destination": "wwwroot/lib/bootstrap",
      "provider": "unpkg"
    },
    {
      "library": "flag-icons@7.1.0",
      "files": [ "css/flag-icons.css", "css/flag-icons.min.css" ],
      "destination": "wwwroot/lib/flag-icons"
    }

  ]
}